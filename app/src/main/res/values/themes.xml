        <item name="android:minHeight">22dip</item>
        <item name="android:maxHeight">22dip</item>
    </style>

    <style name="Theme.HealthInspector" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="colorPrimary">@color/primaryColor</item>
        <item name="colorPrimaryVariant">@color/primaryLightColor</item>
        <item name="colorOnPrimary">@color/primaryLightColor</item>
        <item name="colorSecondary">@color/secondaryColor</item>
        <item name="colorSecondaryVariant">@color/secondaryColor</item>
        <item name="colorOnSecondary">@color/secondaryColor</item>
        <item name="android:statusBarColor" tools:targetApi="l">#000000</item>
        <item name="android:textColor">@color/primaryTextColor</item>
    </style>
